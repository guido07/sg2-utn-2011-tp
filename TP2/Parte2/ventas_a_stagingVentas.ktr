  <step>
    <name>Precios Staging</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>Conexion_TDCStaging</connection>
    <sql>select ve.cod_venta, deve.cod_producto, sum(pr.precio*deve.cantidad) Total_producto 
from productos p 
inner JOIN 

	(select max(pr.`fechadesde`)fecha, pr.cod_producto 
	from precios pr inner join detalle_ventas dv on pr.cod_producto = dv.cod_producto
	inner join ventas v on dv.cod_venta = v.cod_venta
	where pr.`fechadesde`&lt;=v.fecha 
	group by pr.cod_producto) as pmax 

on p.cod_producto = pmax.cod_producto 
inner join precios pr on pmax.cod_producto = pr.cod_producto and pmax.fecha = pr.fechadesde
inner join detalle_ventas deve on deve.cod_producto = pmax.cod_producto
inner join ventas ve on ve.cod_venta = deve.cod_venta
group by 1,2  </sql>
