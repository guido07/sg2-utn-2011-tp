        <jsScript_script>
var fecha_ingreso;
var fecha_cumple;

var var1 = BIRTH_DATE.split(&quot;&#47;&quot;);

var dia1 = parseInt(var1[1],10);
var mes1 = parseInt(var1[0],10);
var anio1 = parseInt(var1[2],10);
var dmax;

if(!isNaN(dia1) &amp;&amp; !isNaN(mes1) &amp;&amp; !isNaN(anio1))
{
       if ((dia1 &lt;= 31) &amp;&amp; (mes1 &lt;= 12) &amp;&amp; (dia1 &gt; 0) &amp;&amp; (mes1 &gt; 0)) 
       {
               switch (mes1) 
               {
                       case 1:dmax = 31;break;
                       case 2: if (anio1 % 4 == 0) dmax = 29; else dmax = 28; break;
                       case 3:dmax = 31;break;
                       case 4:dmax = 30;break;
                       case 5:dmax = 31;break;
                       case 6:dmax = 30;break;
                       case 7:dmax = 31;break;
                       case 8:dmax = 31;break;
                       case 9:dmax = 30;break;
                       case 10:dmax = 31;break;
                       case 11:dmax = 30;break;
                       case 12:dmax = 31;break;
               }
               if (dia1 &lt;= dmax)
               {
				
					fecha_cumple = anio1+&quot;-&quot;+mes1+&quot;-&quot;+dia1;
               }
               else fecha_cumple = &quot;1800-01-01&quot;;
       }
       else fecha_cumple = &quot;1800-01-01&quot;;
}
else fecha_cumple = &quot;1800-01-01&quot;;


var var2 = EMPLOYMENT_DATE.split(&quot;&#47;&quot;);

var dia2 = parseInt(var2[1],10);
var mes2 = parseInt(var2[0],10);
var anio2 = parseInt(var2[2],10);
var dmax;

if(!isNaN(dia2) &amp;&amp; !isNaN(mes2) &amp;&amp; !isNaN(anio2))
{
       if ((dia2 &lt;= 31) &amp;&amp; (mes2 &lt;= 12) &amp;&amp; (dia2 &gt; 0) &amp;&amp; (mes2 &gt; 0)) 
       {
               switch (mes2) 
               {
                       case 1:dmax = 31;break;
                       case 2: if (anio1 % 4 == 0) dmax = 29; else dmax = 28; break;
                       case 3:dmax = 31;break;
                       case 4:dmax = 30;break;
                       case 5:dmax = 31;break;
                       case 6:dmax = 30;break;
                       case 7:dmax = 31;break;
                       case 8:dmax = 31;break;
                       case 9:dmax = 30;break;
                       case 10:dmax = 31;break;
                       case 11:dmax = 30;break;
                       case 12:dmax = 31;break;
               }
               if (dia2 &lt;= dmax)
               {
            
					fecha_ingreso = anio2+&quot;-&quot;+mes2+&quot;-&quot;+dia2;
               }
               else fecha_ingreso = &quot;1800-01-01&quot;;
       }
       else fecha_ingreso = &quot;1800-01-01&quot;;
}
else fecha_ingreso = &quot;1800-01-01&quot;;</jsScript_script>
